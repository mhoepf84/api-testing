		{
			"name": "Create board",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test('Status code is 200', () => {\r",
							"    pm.response.to.have.status(200);\r",
							"})\r",
							"\r",
							"const response = pm.response.json();\r",
							"\r",
							"pm.test('Board is created', () => {\r",
							"    pm.expect(response.name).to.contain('Postman Trello Tests');\r",
							"    pm.expect(response.closed).to.be.false\r",
							"})\r",
							"\r",
							"pm.test('Board is private', () => {\r",
							"    pm.expect(response.prefs.permissionLevel).to.eql('private')\r",
							"})\r",
							"\r",
							"pm.test('Calender is disabled', () => {\r",
							"    pm.expect(response.prefs.switcherViews.find(view => view.viewType === 'Calendar').enabled).to.be.false\r",
							"})\r",
							"\r",
							"pm.collectionVariables.set('boardId', response.id)"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"let boardNumber = pm.collectionVariables.get('boardNumber');\r",
							"\r",
							"if (isNaN(boardNumber)) {\r",
							"    boardNumber = 0\r",
							"}\r",
							"\r",
							"boardNumber++;\r",
							"pm.collectionVariables.set('boardNumber', boardNumber);"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"url": {
					"raw": "{{base_url}}/1/boards/?name=Postman Trello Tests {{boardNumber}}&key={{api_key}}&token={{token}}",
